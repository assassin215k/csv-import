{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "gedmo/doctrine-extensions": "^3.4",
        "league/csv": "^9.7",
        "symfony/console": "6.0.*",
        "symfony/dotenv": "6.0.*",
        "symfony/flex": "^1.17|^2",
        "symfony/framework-bundle": "6.0.*",
        "symfony/proxy-manager-bridge": "6.0.*",
        "symfony/runtime": "6.0.*",
        "symfony/validator": "6.0.*",
        "symfony/yaml": "6.0.*",
        "wickedreports/phpcs-pre-commit-hook": "^0.8.0"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "PHPCodeChecker\\Installer::postInstall",
            "@phpcs-symfony"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "PHPCodeChecker\\Installer::postInstall",
            "@phpcs-symfony"
        ],
        "phpcs-symfony": "php vendor/squizlabs/php_codesniffer/bin/phpcs --config-set installed_paths vendor/escapestudios/symfony2-coding-standard",
        "phpcs": "php vendor/squizlabs/php_codesniffer/bin/phpcs --standard=Symfony"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "3.x-dev",
        "squizlabs/php_codesniffer": "3.5.8",
        "symfony/maker-bundle": "^1.36"
    }
}
